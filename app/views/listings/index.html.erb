<% if @listings.any? %>
  <% @listings.each do |listing| %>
    <%= link_to listing.name, listing_path(listing) %>
    <%= link_to "Add Photos", new_listing_photo_path(listing) %>
  <% end %>
<% else %>
  <p>No listings yet.<p>
<% end %>

<%= link_to "Add a listing", new_listing_path %>


<% if @listings.any? %>
  <% @listings.each do |listing| %>
 		<%= link_to "Edit #{listing.name}", edit_listing_path(listing) %>
    <%= link_to "Delete #{listing.name}", listing_path(listing), method: :delete %>
  <% end %>
<% end %>


